{
  "name": "cs160-online-banking",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:docker": "docker compose run --rm app pnpm test",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "seed": "prisma db seed",
    "prisma:generate": "prisma generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.3.0",
    "@prisma/client": "^6.16.2",
    "@prisma/extension-accelerate": "^2.0.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-form": "^1.23.7",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "@vis.gl/react-google-maps": "^1.5.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.544.0",
    "mini-svg-data-uri": "^1.4.4",
    "motion": "^12.23.24",
    "next": "15.4.6",
    "next-swagger-doc": "^0.4.1",
    "next-themes": "^0.4.6",
    "openai": "^6.7.0",
    "react": "19.1.0",
    "react-currency-input-field": "^4.0.3",
    "react-dom": "19.1.0",
    "swagger-ui-react": "^5.29.4",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.19.17",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-ui-react": "^5.18.0",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-testing-library": "^7.10.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.1",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "prisma": "^6.16.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.40.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@prisma/client",
      "prisma"
    ]
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,cjs,mjs}": [
      "pnpm exec eslint --fix --cache --cache-location node_modules/.cache/eslint"
    ],
    "**/*.{json,md,mdx,css,scss,html,yml,yaml,prisma}": [
      "pnpm exec prettier --write --ignore-unknown"
    ]
  }
}
